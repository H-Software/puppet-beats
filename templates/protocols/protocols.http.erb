  http:
    ports: [<%= @http_ports %>]
    <% if @hide_keywords %>
    hide_keywords: [
    <% @hide_keywords.each do |kw| -%>
      "<%= kw %>",
    <% end -%>
    ]
    <% end -%>
    <% if  @send_headers %>
    send_headers: [
    <% @send_headers.each do |header| -%>
      "<%= header %>",
    <% end -%>
    ]
    <% end -%>
    real_ip_header: "<%= @real_ip_header %>"
    split_cookie: <% @split_cookie %>

<% @prospectors.each do |key,prospector_hash| %>
    -
      paths:<% prospector_hash['paths'].each do |path| %>
        - <%= path %>
      <%- end %>
      fields:<% prospector_hash['fields'].each do |key, value| %>
        <%= key -%>: <%= value %>
      <%- end %>
      encoding: plain
      input_type: log
      #ignore_older: 24h
      #document_type: log
      #scan_frequency: 10s
      #harvester_buffer_size: 16384
      tail_files: true
      backoff: 1s
      max_backoff: 10s
      backoff_factor: 2
      partial_line_waiting: 5s
      force_close_files: false

<% end -%>
